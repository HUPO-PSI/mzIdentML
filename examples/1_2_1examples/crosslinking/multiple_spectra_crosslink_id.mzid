            <SearchDatabaseRef searchDatabase_ref="database_id"></SearchDatabaseRef>
        </SpectrumIdentification>
    </AnalysisCollection>

    <AnalysisProtocolCollection>
        <cvParam accession="MS:XXXXXXX" cvRef="PSI-MS"
                 name="multiple spectra cross-linking search"/>
        <SpectrumIdentificationProtocol analysisSoftware_ref="fdr_prog_id"
                                        id="SearchProtocol_HCD">
            <!-- somewhere here come the b and y ion cv-terms -->
            </SpectrumIdentificationProtocol>

                  <SpectrumIdentificationProtocol analysisSoftware_ref="fdr_prog_id"
