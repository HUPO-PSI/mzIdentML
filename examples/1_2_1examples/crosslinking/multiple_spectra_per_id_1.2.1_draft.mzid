                    <cvParam cvRef="PSI-MS" accession="MS:1002510" name="cross-link acceptor" value="0"/>
                </SearchModification>

                <SearchModification fixedMod="true" massDelta="158.00377" residues=".">
                    <SpecificityRules>
                        <cvParam cvRef="PSI-MS" accession="MS:1002057" name="modification specificity protein N-term"/>
                    </SpecificityRules>
                    <cvParam accession="UNIMOD:1842" cvRef="UNIMOD" name="Xlink:DSS0"/>
                    <cvParam cvRef="PSI-MS" accession="MS:1002509" name="cross-link donor" value="0"/>
                </SearchModification>

                <SearchModification fixedMod="true" massDelta="158.00377" residues=".">
                    <SpecificityRules>
                        <cvParam cvRef="PSI-MS" accession="MS:1002057" name="modification specificity protein N-term"/>
                    </SpecificityRules>
                    <cvParam accession="UNIMOD:1842" cvRef="UNIMOD" name="Xlink:DSS0"/>
