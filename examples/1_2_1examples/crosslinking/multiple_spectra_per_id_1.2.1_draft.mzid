                <cvParam accession="MS:XXXXXX" name="UNIMOD derivative code" value="T" cvRef="PSI-MS" />
                <cvParam cvRef="PSI-MS" accession="MS:XXXXXX" name="cross-link stub"/>
            </Modification>
