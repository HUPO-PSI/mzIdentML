            <InputSpectra spectraData_ref="peaklist_id"/>
            <SearchDatabaseRef searchDatabase_ref="database_id"/>
        </SpectrumIdentification>
        <SpectrumIdentification spectrumIdentificationProtocol_ref="SearchProtocol_MS3"
